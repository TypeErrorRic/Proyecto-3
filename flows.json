    {
        "id": "fa939748703619c4",
        "type": "function",
        "z": "0c43ec2564d8737a",
        "name": "function 3",
        "func": "const payload = msg.payload;\nconst claves = Object.keys(payload);\n\nif (claves.length === 2 && claves[0] === 'recaptcha') {\n    msg.payload = {\n        clave: `${msg.payload.recaptcha}`,\n        correo: `${msg.payload.correo}`\n    };\n    return msg;\n}\n\nreturn null;",
